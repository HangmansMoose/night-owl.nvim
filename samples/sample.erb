<!DOCTYPE html>
<html>
<head>
  <title>Foobar</title>
</head>
<body>
  <% foo = "Lorem Ipsum" %>
  <% bar = 123 %>
  <% baz = [1, 2, 3, 4, 5] %>
  <% qux = {"foo" => "bar", "baz" => "qux"} %>

  <h1><%= foo %></h1>
  <p><%= bar %></p>

  <ul>
    <% baz.each do |i| %>
      <li><%= i %></li>
    <% end %>
  </ul>

  <dl>
    <% qux.each do |key, value| %>
      <dt><%= key %></dt>
      <dd><%= value %></dd>
    <% end %>
  </dl>

  <% if qux.key?("foo") %>
    <p>Found foo: <%= qux["foo"] %></p>
  <% else %>
    <p>Did not find foo</p>
  <% end %>

  <% case bar %>
  <% when 123 %>
    <p>Bar is 123</p>
  <% else %>
    <p>Bar is not 123</p>
  <% end %>
</body>
</html>
